#![enable(implicit_some)]
#![enable(unwrap_newtypes)]
#![enable(unwrap_variant_newtypes)]
(
    default_album_art_path: None,
    show_song_table_header: true,
    draw_borders: true,
    format_tag_separator: " | ",
    browser_column_widths: [20, 38, 42],
    background_color: None,
    text_color: None,
    header_background_color: None,
    modal_background_color: None,
    modal_backdrop: false,
    preview_label_style: (fg: "#493B49"),
    preview_metadata_group_style: (fg: "#493B49", modifiers: "Bold"),
    tab_bar: (
        enabled: true,
        active_style: (fg: "#0b0c12", bg: "#38415E", modifiers: "Bold"),
        inactive_style: (),
    ),
    highlighted_item_style: (fg: "#38415E", modifiers: "Bold"),
    current_item_style: (fg: "#0b0c12", bg: "#38415E", modifiers: "Bold"),
    borders_style: (fg: "#38415E"),
    highlight_border_style: (fg: "#38415E"),
    symbols: (
        song: "S",
        dir: "D",
        playlist: "P",
        marker: "->",
        ellipsis: "...",
        song_style: None,
        dir_style: None,
        playlist_style: None,
    ),
    level_styles: (
        info: (fg: "#38415E", bg: "#0b0c12"),
        warn: (fg: "#493B49", bg: "#0b0c12"),
        error: (fg: "#2C324D", bg: "#0b0c12"),
        debug: (fg: "#2C3D62", bg: "#0b0c12"),
        trace: (fg: "#37486D", bg: "#0b0c12"),
    ),
    progress_bar: (
        symbols: ["[", "-", ">", " ", "]"],
        track_style: (fg: "#0b0c12"),
        elapsed_style: (fg: "#38415E"),
        thumb_style: (fg: "#38415E", bg: "#0b0c12"),
    ),
    scrollbar: (
        symbols: ["│", "█", "▲", "▼"],
        track_style: (),
        ends_style: (),
        thumb_style: (fg: "#38415E"),
    ),
    song_table_format: [
        (
            prop: (kind: Property(Artist),
                default: (kind: Text("Unknown"))
            ),
            width: "20%",
        ),
        (
            prop: (kind: Property(Title),
                default: (kind: Text("Unknown"))
            ),
            width: "35%",
        ),
        (
            prop: (kind: Property(Album), style: (fg: "#c7a9a0"),
                default: (kind: Text("Unknown Album"), style: (fg: "#c7a9a0"))
            ),
            width: "30%",
        ),
        (
            prop: (kind: Property(Duration),
                default: (kind: Text("-"))
            ),
            width: "15%",
            alignment: Right,
        ),
    ],
    components: {},
    layout: Split(
        direction: Vertical,
        panes: [
            (
                pane: Pane(Header),
                size: "2",
            ),
            (
                pane: Pane(Tabs),
                size: "3",
            ),
            (
                pane: Pane(TabContent),
                size: "100%",
            ),
            (
                pane: Pane(ProgressBar),
                size: "1",
            ),
        ],
    ),
    header: (
        rows: [
            (
                left: [
                    (kind: Text("["), style: (fg: "#493B49", modifiers: "Bold")),
                    (kind: Property(Status(StateV2(playing_label: "Playing", paused_label: "Paused", stopped_label: "Stopped"))), style: (fg: "#493B49", modifiers: "Bold")),
                    (kind: Text("]"), style: (fg: "#493B49", modifiers: "Bold"))
                ],
                center: [
                    (kind: Property(Song(Title)), style: (modifiers: "Bold"),
                        default: (kind: Text("No Song"), style: (modifiers: "Bold"))
                    )
                ],
                right: [
                    (kind: Property(Widget(ScanStatus)), style: (fg: "#38415E")),
                    (kind: Property(Widget(Volume)), style: (fg: "#38415E"))
                ]
            ),
            (
                left: [
                    (kind: Property(Status(Elapsed))),
                    (kind: Text(" / ")),
                    (kind: Property(Status(Duration))),
                    (kind: Text(" (")),
                    (kind: Property(Status(Bitrate))),
                    (kind: Text(" kbps)"))
                ],
                center: [
                    (kind: Property(Song(Artist)), style: (fg: "#493B49", modifiers: "Bold"),
                        default: (kind: Text("Unknown"), style: (fg: "#493B49", modifiers: "Bold"))
                    ),
                    (kind: Text(" - ")),
                    (kind: Property(Song(Album)),
                        default: (kind: Text("Unknown Album"))
                    )
                ],
                right: [
                    (
                        kind: Property(Widget(States(
                            active_style: (fg: "#c7a9a0", modifiers: "Bold"),
                            separator_style: (fg: "#c7a9a0")))
                        ),
                        style: (fg: "#8b7670")
                    ),
                ]
            ),
        ],
    ),
    browser_song_format: [
        (
            kind: Group([
                (kind: Property(Track)),
                (kind: Text(" ")),
            ])
        ),
        (
            kind: Group([
                (kind: Property(Artist)),
                (kind: Text(" - ")),
                (kind: Property(Title)),
            ]),
            default: (kind: Property(Filename))
        ),
    ],
    lyrics: (
        timestamp: false
    )
)
